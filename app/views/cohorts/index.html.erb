

<h1>All Cohorts</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Start Date</th>
        <th scope="col">End Date</th>
        <th scope="col">Show</th>
        <th scope="col">Edit</th>
        <th scope="col">Delete</th>
      </tr>
    </thead>
    <tbody>
    <% @cohorts.each do |cohort| %>
      <tr>
        <th scope="row"><%= cohort.id %></th>
        <td><%= cohort.name %></d>
        <td><%= cohort.start_date %></td>
        <td><%= cohort.end_date %></td>
        <td><%= link_to "Show Cohort", cohort %></td>
        <td><%= link_to "Edit Cohort", edit_cohort_path(cohort) %></td>
        <td> delete </td>
      </tr>
      <% end %>
    </tbody>
  </table>
