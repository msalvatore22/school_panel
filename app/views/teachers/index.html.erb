<h1>All Teachers</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Age</th>
        <th scope="col">Salary</th>
        <th scope="col">Highest Completed Education</th>
      </tr>
    </thead>
    <tbody>
    <% @teachers.each do |teacher| %>
      <tr>
        <th scope="row"><%= teacher.id %></th>
        <td><%= teacher.first_name %></d>
        <td><%= teacher.last_name %></td>
        <td><%= teacher.age %></td>
        <td><%= teacher.salary %></td>
        <td><%= teacher.highest_completed_education %></td>
      </tr>
    </tbody>
  </table>
<% end %>